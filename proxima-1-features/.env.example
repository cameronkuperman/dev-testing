# Gemini Live API Configuration
GEMINI_API_KEY=your_gemini_api_key_here

# Model Selection
# Native audio models (more natural voice):
# - gemini-2.5-flash-preview-native-audio-dialog
# - gemini-2.5-flash-exp-native-audio-thinking-dialog
# Half-cascade models (better reliability):
# - gemini-live-2.5-flash-preview
# - gemini-2.0-flash-live-001
GEMINI_MODEL=gemini-2.5-flash-preview-native-audio-dialog

# Voice Configuration
# Available voices: Aoede, Charon, Fenrir, Kore, Puck (default)
GEMINI_VOICE=Aoede

# WebSocket Configuration
NEXT_PUBLIC_WS_ENDPOINT=ws://localhost:3000/api/voice
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Optional: Session Configuration
SESSION_TIMEOUT_MS=1800000  # 30 minutes
MAX_AUDIO_DURATION_MS=300000  # 5 minutes per interaction

# Optional: Development Settings
LOG_LEVEL=info
ENABLE_AUDIO_RECORDING=false